<p id="notice"><%= notice %></p>

<h1>Ethograms</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ethograms.each do |ethogram| %>
      <tr>
        <td><%= ethogram.name %></td>
        <td><%= link_to 'Show', ethogram %></td>
        <td><%= link_to 'Edit', edit_ethogram_path(ethogram) %></td>
        <td><%= link_to 'Destroy', ethogram, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ethogram', new_ethogram_path %>
